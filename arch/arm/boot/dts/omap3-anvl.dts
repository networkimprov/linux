/dts-v1/;

#include "omap36xx.dtsi"

/ {
	model = "Nework Improv ANVL";
	compatible = "ti,omap3-anvl", "ti,omap3";

	cpus {
		cpu@0 {
			cpu0-supply = <&vcc>;
		};
	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>;	/* 512 MB */
	};

	fixed_1v8_io: fixed_1v8_io {
		compatible = "regulator-fixed";
		regulator-name = "1v8_io";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-boot-on;
	};

	fixed_3v3_sys: fixed_3v3_sys {
		compatible = "regulator-fixed";
		regulator-name = "3v3_sys";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
	};

	fixed_wlan: fixed_wlan {
		compatible = "regulator-fixed";
		regulator-name = "fixed_wlan";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		enable-active-high;
		gpio = <&gpio5 27 0>;	/* gpio155 */
	};

	leds {
		compatible = "gpio-leds";
		heartbeat {
			label = "anvl::cpu0";
			gpios = <&gpio6 15 0>;	/* gpio175 */
			linux,default-trigger = "cpu0";
		};
	};

	sound {
		compatible = "ti,omap-twl4030";
		ti,model = "anvl";

		ti,mcbsp = <&mcbsp2>;
		ti,codec = <&twl_audio>;
		ti,audio-routing =
			"Handsfree Spk", "HFL";
	};
};

&omap3_pmx_core {
	mmc1_pins: pinmux_mmc1_pins {
		pinctrl-single,pins = <
			0x114 0x18	/* sdmmc1_clk.sdmmc1_clk OUTPUT_PULLUP | MODE0 */
			0x116 0x18	/* sdmmc1_cmd.sdmmc1_cmd OUTPUT_PULLUP | MODE0 */
			0x118 0x118	/* sdmmc1_dat0.sdmmc1_dat0 INPUT_PULLUP | MODE0 */
			0x11a 0x118	/* sdmmc1_dat1.sdmmc1_dat1 INPUT_PULLUP | MODE0 */
			0x11c 0x118	/* sdmmc1_dat2.sdmmc1_dat2 INPUT_PULLUP | MODE0 */
			0x11e 0x118	/* sdmmc1_dat3.sdmmc1_dat3 INPUT_PULLUP | MODE0 */
		>;
	};

	mmc2_pins: pinmux_mmc2_pins {
		pinctrl-single,pins = <
			0xb6 0x3e1c	/* dss_data5.gpio_75 gpio75 OFF* | OUTPUT | MODE4 EMMC_RESET */
			0x128 0x118	/* sdmmc2_clk.sdmmc2_clk INPUT_PULLUP | MODE0 */
			0x12a 0x118	/* sdmmc2_cmd.sdmmc2_cmd INPUT_PULLUP | MODE0 */
			0x12c 0x100	/* sdmmc2_dat0.sdmmc2_dat0 INPUT | MODE0 */
			0x12e 0x100	/* sdmmc2_dat1.sdmmc2_dat1 INPUT | MODE0 */
			0x130 0x100	/* sdmmc2_dat2.sdmmc2_dat2 INPUT | MODE0 */
			0x132 0x100	/* sdmmc2_dat3.sdmmc2_dat3 INPUT | MODE0 */
			0x134 0x100	/* sdmmc2_dat4.sdmmc2_dat4 INPUT | MODE0 */
			0x136 0x100	/* sdmmc2_dat5.sdmmc2_dat5 INPUT | MODE0 */
			0x138 0x100	/* sdmmc2_dat6.sdmmc2_dat6 INPUT | MODE0 */
			0x13a 0x100	/* sdmmc2_dat7.sdmmc2_dat7 INPUT | MODE0 */
		>;
	};

	mmc3_active_pin: pinmux_mmc3_active_pin {
		pinctrl-single,pins = <
			0x5b6 0x11a	/* etk_d5.sdmmc3_dat1 INPUT_PULLUP | MODE2 */
		>;
	};

	mmc3_idle_pin: pinmux_mmc3_idle_pin {
		pinctrl-single,pins = <
			0x5b6 0x4104	/* etk_d5.gpio19 WAKEUP | INPUT | MODE4 */
		>;
		gpios = <&gpio1 19 0>;	/* gpio19 */
	};

	mmc3_pins: pinmux_mmc3_pins {
		pinctrl-single,pins = <
			0x15a 0xf04	/* mcbsp4_fsx.gpio_155 OFF_OUTPUT_HIGH | MODE4 WLAN_PDN */
			0x1a4 0x104	/* mcspi1_cs3.gpio_177 INPUT | MODE4 WLAN_SLEEP */
			0x156 0x4	/* mcbsp4_dr.gpio_153 MODE4 WLAN_WKUP */
			0x1a2 0x4	/* mcspi1_cs2.gpio_176 MODE4 WLAN_EEPROM_WP */
			0x5a8 0x331a	/* etk_clk.sdmmc3_clk OFF_PULLUP | INPUT_PULLUP | MODE2 */
			0x5aa 0x331a	/* etk_ctl.sdmmc3_cmd OFF_PULLUP | INPUT_PULLUP | MODE2 */
			0x5b2 0x331a	/* etk_d3.sdmmc3_dat3 OFF_PULLUP | INPUT_PULLUP | MODE2 */
			0x5b4 0x331a	/* etk_d4.sdmmc3_dat0 OFF_PULLUP | INPUT_PULLUP | MODE2 */
			0x5b8 0x331a	/* etk_d6.sdmmc3_dat2 OFF_PULLUP | INPUT_PULLUP | MODE2 */
		>;
	};

	uart1_pins: pinmux_uart1_pins {
		pinctrl-single,pins = <
			0x150 0x108	/* uart1_cts.uart1_cts INPUT_PULLDOWN | MODE0 */
			0x14e 0x10	/* uart1_rts.uart1_rts OUTPUT | MODE0 */
			0x152 0x4100	/* uart1_rx.uart1_rx WAKEUP | INPUT | MODE0 */
			0x14c 0x0	/* uart1_tx.uart1_tx OUTPUT | MODE0 */
		>;
		interrupts = <72>;
	};

	uart3_pins: pinmux_uart3_pins {
		pinctrl-single,pins = <
			0x16e 0x4100	/* uart3_rx_irrx.uart3_rx_irrx WAKEUP | INPUT | MODE0 */
			0x170 0x0	/* uart3_tx_irtx.uart3_tx_irtx OUTPUT | MODE0 */
		>;
		interrupts = <74>;
	};
};

&omap3_pmx_wkup {
	wlan_host_wkup: pinmux_wlan_host_wkup_pins {
		pinctrl-single,pins = <
			0x1a 0x411c	/* sys_clkout1.gpio_10 WAKEUP |INPUT_PULLUP | MODE4 WLAN_HOST_WKUP */
		>;
	};
};

&gpmc {
	ranges = <0 0 0x00000000 0x20000000>;

	nand@0,0 {
		linux,mtd-name = "micron,mt29c4g96maz";

		reg = <0 0 0>;
		nand-bus-width = <16>;
		ti,nand-ecc-opt = "bch8";

		gpmc,sync-clk-ps = <0>;
		gpmc,cs-on-ns = <0>;
		gpmc,cs-rd-off-ns = <44>;
		gpmc,cs-wr-off-ns = <44>;
		gpmc,adv-on-ns = <6>;
		gpmc,adv-rd-off-ns = <34>;
		gpmc,adv-wr-off-ns = <44>;
		gpmc,we-off-ns = <40>;
		gpmc,oe-off-ns = <54>;
		gpmc,access-ns = <64>;
		gpmc,rd-cycle-ns = <82>;
		gpmc,wr-cycle-ns = <82>;
		gpmc,wr-access-ns = <40>;
		gpmc,wr-data-mux-bus-ns = <0>;

		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "MLO";
			reg = <0 0x80000>;
		};
		partition@80000 {
			label = "U-Boot";
			reg = <0x80000 0x1c0000>;
		};
		partition@240000 {
			label = "U-Boot Environment";
			reg = <0x240000 0x20000>;
		};
		partition@260000 {
			label = "Kernel";
			reg = <0x260000 0x600000>;
		};
		partition@860000 {
			label = "Initrd";
			reg = <0x860000 0x1000000>;
		};
		partition@1880000 {
			label = "MTD Oops";
			reg = <0x1860000 0x100000>;
		};
		partition@1980000 {
			label = "Filesystem";
			reg = <0x1960000 0x1e6a0000>;
		};
	};
};

&i2c1 {
	clock-frequency = <400000>;

	twl: twl@48 {
		reg = <0x48>;
		interrupts = <7>;	/* SYS_NIRQ */
		interrupt-parent = <&intc>;

		twl_audio: audio {
			compatible = "ti,twl4030-audio";
			codec {
			};
		};
	};

	bq24190: bq24190@6b {
		compatible = "ti,bq24190";
		reg = <0x6b>;
		interrupts = <24 IRQ_TYPE_EDGE_RISING>;
		interrupt-parent = <&gpio5>;
	};
};

/include/ "twl4030.dtsi"

&i2c2 {
	clock-frequency = <400000>;

	pca9632: pca9632 {
		compatible = "nxp,pca9632", "nxp,pca963x";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x62>;

		red@0 {
			label = "red";
			reg = <0>;
			linux,default-trigger = "none";
		};
		green@1 {
			label = "green";
			reg = <1>;
			linux,default-trigger = "none";
		};
		blue@2 {
			label = "blue";
			reg = <2>;
			linux,default-trigger = "none";
		};
		unused@3 {
			label = "unused";
			reg = <3>;
			linux,default-trigger = "none";
		};
	};
};

&mmc1 {
	vmmc-supply = <&vmmc1>;
	bus-width = <4>;
	pinctrl-names = "default";
	pinctrl-0 = <&mmc1_pins>;
};

&mmc2 {
	vmmc-supply = <&fixed_1v8_io>;
	ti,non-removable;
	bus-width = <8>;
	pinctrl-names = "default";
	pinctrl-0 = <&mmc2_pins>;
};

&mmc3 {
	vmmc-supply = <&fixed_wlan>;
	non-removable;
	bus-width = <4>;
	cap-power-off-card;
	ti,cirq-gpio = <&gpio1 19 0>;	/* gpio19 */
	pinctrl-names = "default", "active", "idle";
	pinctrl-0 = <&mmc3_pins &wlan_host_wkup>;
	pinctrl-1 = <&mmc3_active_pin>;
	pinctrl-2 = <&mmc3_idle_pin>;
};

&uart1 {
       pinctrl-names = "default";
       pinctrl-0 = <&uart1_pins>;
};

&uart2 {
	status = "disabled";
};

&uart3 {
       pinctrl-names = "default";
       pinctrl-0 = <&uart3_pins>;
};

&uart4 {
	status = "disabled";
};

&usbhshost {
	status = "disabled";
};

&usb_otg_hs {
	interface-type = <0>;
	usb-phy = <&usb2_phy>;
	mode = <3>;
	power = <50>;
};
